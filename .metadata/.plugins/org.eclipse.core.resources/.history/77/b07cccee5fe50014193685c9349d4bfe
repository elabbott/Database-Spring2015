		guiFrame1.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		guiFrame1.setTitle("Space Monkey Movie Service");
		guiFrame1.setSize(300,250);
		guiFrame1.setLocationRelativeTo(null);
		
			
	}
}
